{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    },
    "extensions": {
        "queues": {
            "maxPollingInterval": "00:00:03",
            "visibilityTimeout": "00:00:02",
            "batchSize": 1,
            "maxDequeueCount": 2,
            "messageEncoding": "base64",
            "newBatchThreshold": 1
        }
    }
}